@model Entidades.Parceiro

@{
    ViewBag.Title = "Excluindo parceiro: " + Model.nomeStr;
}

<h2>Excluindo parceiro: @Model.nomeStr</h2>

<h3>Tem certeza que deseja excluir este parceiro?</h3>
<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.nomeStr)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.nomeStr)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.imagemStr)
        </dt>

        <dd>
            <img width="200" height="200" src="~/Content/images/parceiros/@Model.imagemStr" />
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.AtivoBool)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.AtivoBool)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.DataCadastroDate)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.DataCadastroDate)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Excluir" class="btn btn-default" /> |
            @Html.ActionLink("Voltar", "Index")
        </div>
    }
</div>
